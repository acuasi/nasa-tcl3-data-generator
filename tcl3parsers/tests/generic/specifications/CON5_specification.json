{
    "basic": {
        "uvin": {
            "match": {
                "type": "str",
                "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$",
                "maxLength": 36,
                "minLength": 36
            }
        },
        "gufi": {
            "match": {
                "type": "str",
                "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$",
                "maxLength": 36,
                "minLength": 36
            }
        },
        "submitTime": {
            "match": {
                "type": "str",
                "pattern": "^2[0-9][0-9][0-9]-[01][0-9]-[0123][0-9]T[012][0-9]:[012345][0-9]:[012345][0-9]\\.[0-9][0-9][0-9][0-9]*Z{0,1}$"
            }
        },
        "ussName": {
            "match": {
                "type": "str"
            }
        },
        "ussInstanceID": {
            "match": {
                "type": "str",
                "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$",
                "maxLength": 36,
                "minLength": 36
            }
        }
    },
    "fType": {
        "match": {
            "type": "str",
            "enum": [
                "CON5"
            ]
        }
    },
    "distFromBoundary": {
        "match": {
            "type": "list",
            "children": {
                "match": {
                    "type": "dict",
                    "children": {
                        "ts": {
                            "match": {
                                "type": "str",
                                "pattern": "^2[0-9][0-9][0-9]-[01][0-9]-[0123][0-9]T[012][0-9]:[012345][0-9]:[012345][0-9]\\.[0-9][0-9][0-9][0-9]*Z{0,1}$"
                            }
                        },
                        "distFromBoundary_ft": {
                            "match": {
                                "type": "float|int"
                            }
                        }
                    }
                }
            }
        }
    },
    "unauthorizedAirspaceStatus": {
        "match": {
            "type": "list",
            "children": {
                "match": {
                    "type": "dict",
                    "children": {
                        "ts": {
                            "match": {
                                "type": "str",
                                "pattern": "^2[0-9][0-9][0-9]-[01][0-9]-[0123][0-9]T[012][0-9]:[012345][0-9]:[012345][0-9]\\.[0-9][0-9][0-9][0-9]*Z{0,1}$"
                            }
                        },
                        "unauthorizedAirspaceStatus_nonDim": {
                            "match": {
                                "type": "int",
                                "enum": [
                                    0,
                                    1
                                ]
                            }
                        }
                    }
                }
            }
        }
    }
}